############### SO PODE HAVER UM CONTROLADOR NO FICHEIRO##############
##SELECIONAR SOURCE E CLICAR##


import maya.cmds as cmds
import pymel.core as pm

selection = cmds.ls(sl=True)   
selectionStr = ' '.join(selection)

cmds.select( "*:CC_BlendShapes")
cmds.setKeyframe()

cmds.selectKey("{0}_{1}".format(selectionStr,"browInnerUp"), replace=True, keyframe=True)  
cmds.copyKey()
cmds.pasteKey( "*:CC_BlendShapes", attribute='browInnerUp' )

cmds.selectKey("{0}_{1}".format(selectionStr,"browDownLeft"), replace=True, keyframe=True)  
cmds.copyKey()
cmds.pasteKey( "*:CC_BlendShapes", attribute='browDownLeft' )

cmds.selectKey("{0}_{1}".format(selectionStr,"browDownRight"), replace=True, keyframe=True)  
cmds.copyKey()
cmds.pasteKey( "*:CC_BlendShapes", attribute='browDownRight' )

